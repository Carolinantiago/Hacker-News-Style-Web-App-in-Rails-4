<%@links.each_with_index do |link, index| %>
  <div class="link row clearfix">
    <h2 class="title">
      <%= link_to link.title, link %><br>
      <small class="author" id="so_este_<%=index%>">Submitted <%= time_ago_in_words(link.created_at) %> by <%= link.user.email %></small>
    </h2>
  </div>
<% end %>
